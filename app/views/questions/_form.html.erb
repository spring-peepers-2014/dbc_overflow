<%= form_for @question do |f| %>
  <%= f.label :question_name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :your_question %><br>
  <%= f.text_area :body %><br>
  <%= f.label :tag %><br>
  <%= collection_select(:body, :tags, Tag.all, :id, :name) %><br><br>
  <%= f.submit "submit question!"%>
<% end %>

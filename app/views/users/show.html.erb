<h2><%= @user.name %>'s Profile</h3><br>
<p>
Email: <%= @user.email %><br>
Bio: <%= @user.bio %>
</p>
<h4><%= @user.name %>'s Questions</h4>
  <% @user.questions.each do |question | %>
    <%= link_to(question.name, question_path(question)) %><br>
  <% end %>

<% if session[:user] == @user %>
<%= link_to "Edit Profile", edit_user_path(@user) %>
<% end %>

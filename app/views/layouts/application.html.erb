<!DOCTYPE html>
<html>
<head>
  <title>DbcOverflow</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag    "normalize", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
      <div class="main-nav">

        <h1 class="logo"><a href="/"><span id="dbc">dbc</span><span id="overflow">Overflow</a></span></h1>
          <div class="nav_links">
            <ul>
              <li><%= link_to 'signup', new_user_path %></li>
              <li><%=link_to 'login', user_login_path %></li>
              <li>your profile</li>
            </ul>
          </div>
      </div>

      <div class="child_nav">
          <h1 class="logo-larger"><a href="/"><span id="dbc">dbc</span><span id="overflow">Overflow</a></span></h1>
        <ul>
          <li id="nav-questions">Questions</li>
          <li id="nav-tags">Tags</li>
          <li id="nav-users">Users</li>
          <li id="nav-ask">Ask Question</li>
        </ul>
      </div>

      <div id="login_div">

      </div>

<%= yield %>

</body>
</html>
